<?xml version="1.0" encoding="utf-8"?>
<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>Sales report provides an overview of a site's sales performance over a period of time</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="VirtoCommerce">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=localhost;Initial Catalog=VirtoCommerce</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>b09a048a-5b02-4627-ae1a-7d9a2c3eaa79</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="SalesData">
      <Query>
        <DataSourceName>VirtoCommerce</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@group">
            <Value>=Parameters!GroupBy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
  count(*) as count_order
  , SUM(ShippingTotal) AS Sum_ShippingTotal
  ,SUM(TaxTotal) AS Sum_TaxTotal
  ,SUM(Subtotal) AS Sum_Subtotal
  ,SUM(Total) AS Sum_Total
  ,SUM(DiscountAmount) AS Sum_DiscountAmount
  ,COUNT(LineItemId) AS Count_LineItemId
 , period
 , BillingCurrency
 from
 (select OrderForm.ShippingTotal
  ,OrderGroup.BillingCurrency
  ,OrderForm.TaxTotal
  ,OrderForm.Subtotal
  ,OrderForm.Total
  ,OrderForm.DiscountAmount
  ,LineItem.LineItemId
  ,case 
    when @group='day' then convert(varchar(10), orderform.Created, 126)
    when @group='month' then right(convert(varchar(11), orderform.Created, 113), 8)
    when @group='year' then convert(varchar(4), orderform.Created, 126)
 end as period
FROM OrderForm 
  INNER JOIN LineItem ON OrderForm.OrderFormId = LineItem.OrderFormId 
  INNER JOIN OrderGroup ON OrderForm.OrderGroupId = OrderGroup.OrderGroupId
WHERE
  OrderGroup.Created &gt;= @StartDate
  AND OrderGroup.Created &lt;= @EndDate
  AND (OrderGroup.Status = N'Completed')
) as tbl
group by period, BillingCurrency</CommandText>
        <Timeout>6</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
