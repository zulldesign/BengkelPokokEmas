<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>Report is used to determine which products are running low in inventory.  If a product's inventory is less than its reorder minimum quantity, it will be included in this report.</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="VirtoCommerce">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=localhost;Initial Catalog=VirtoCommerce</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>b09a048a-5b02-4627-ae1a-7d9a2c3eaa79</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="StockData">
      <Query>
        <DataSourceName>VirtoCommerce</DataSourceName>
        <CommandText>SELECT     FulfillmentCenter.Name as  FulfillmentCenter, Inventory.Sku as ItemCode, Inventory.InStockQuantity, Inventory.ReorderMinQuantity, Item.Name AS ItemName
FROM         FulfillmentCenter INNER JOIN
                      Inventory ON FulfillmentCenter.FulfillmentCenterId = Inventory.FulfillmentCenterId INNER JOIN
                      Item ON Inventory.Sku = Item.Code
where Inventory.InStockQuantity &lt;  Inventory.ReorderMinQuantity
order by FulfillmentCenter.Name, Item.Name</CommandText>
        <Timeout>18</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="FulfillmentCenter">
          <DataField>FulfillmentCenter</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ItemCode">
          <DataField>ItemCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="InStockQuantity">
          <DataField>InStockQuantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ReorderMinQuantity">
          <DataField>ReorderMinQuantity</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="ItemName">
          <DataField>ItemName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
