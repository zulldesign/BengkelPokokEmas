<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <Description>Report provides the shipping method, number of orders and total shipping cost over a period of time</Description>
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="VirtoCommerce">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=localhost;Initial Catalog=VirtoCommerce</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>b09a048a-5b02-4627-ae1a-7d9a2c3eaa79</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="StockData">
      <Query>
        <DataSourceName>VirtoCommerce</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT     Shipment.ShippingMethodName, SUM(Shipment.ShippingCost) AS ShippingCost, COUNT(OrderForm.OrderGroupId) AS OrdersCount
FROM         Shipment INNER JOIN
                      OrderForm ON Shipment.OrderFormId = OrderForm.OrderFormId
where Shipment.Created &gt;=@StartDate
	and Shipment.Created &lt;=@EndDate
GROUP BY Shipment.ShippingMethodName</CommandText>
        <Timeout>18</Timeout>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ShippingMethodName">
          <DataField>ShippingMethodName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ShippingCost">
          <DataField>ShippingCost</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="OrdersCount">
          <DataField>OrdersCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
